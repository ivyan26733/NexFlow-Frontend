# Copy this file to .env.local and fill in your values
# .env.local is gitignored and should never be committed

# URL of the Nexflow backend API (Spring Boot)
# Local development: http://localhost:8080
# Production: your Railway deployment URL
NEXT_PUBLIC_API_URL=http://localhost:8080

# WebSocket URL for live execution events in Studio
# Local development: http://localhost:8080/ws
# Production: same Railway URL with /ws appended
NEXT_PUBLIC_WS_URL=http://localhost:8080/ws
